<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ServletInicial</servlet-name>
        <servlet-class>servlets.ServletInicial</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletClientes</servlet-name>
        <servlet-class>servlets.ServletClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientesInicio</servlet-name>
        <servlet-class>servlets.Clientes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletInicial</servlet-name>
        <url-pattern>/inicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletClientes</servlet-name>
        <url-pattern>/sclientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientesInicio</servlet-name>
        <url-pattern>/clientes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>sclientes</welcome-file>
    </welcome-file-list>
</web-app>
